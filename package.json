{
  "name": "react-post-css-modules",
  "version": "1.0.0",
  "description": "A boilerplate for react 16, post-css, hot-loader and css-modules",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "build:prod": "NODE_ENV=production && cp index.html ./build && webpack -p --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pouria-bashar/react-post-css-modules"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "post-css",
    "css-modules",
    "jest"
  ],
  "author": "Pouria Bashardoust <pouria.bashardoust@gmail.com> (http://github.com/pouria-bashar)",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.15.3",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-xo-react": "^0.14.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "21.2.1",
    "postcss-hexrgba": "^1.0.0",
    "postcss-loader": "2.0.8",
    "precss": "2.0.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-hot-loader": "3.1.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "16.1.0",
    "style-loader": "0.19.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link-http": "^1.2.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "react-apollo": "^2.0.1",
    "react-router": "^4.2.0"
  }
}
